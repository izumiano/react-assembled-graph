		"@biomejs/biome": "2.3.10",
		"@pbr1111/vite-plugin-biome": "^1.1.3",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"globals": "^16.5.0",
		"typescript": "~5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-wasm": "^3.5.0"
	}
